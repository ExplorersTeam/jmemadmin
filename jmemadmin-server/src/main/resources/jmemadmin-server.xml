<configuration>
	<!-- SockIOPool Configuration Items -->
	<property>
		<name>jmemadmin.sockiopool.failover</name>
		<value>true</value>
	</property>
	<property>
		<name>jmemadmin.sockiopool.init.connections</name>
		<value>10</value>
	</property>
	<property>
		<name>jmemadmin.sockiopool.min.connections</name>
		<value>5</value>
	</property>
	<property>
		<name>jmemadmin.sockiopool.max.connections</name>
		<value>250</value>
	</property>
	<property>
		<name>jmemadmin.sockiopool.maint.sleep.ms</name>
